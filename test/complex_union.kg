
union Test {
    a: i32,
    b: i8
}

pub fun main() -> i32 {
    let test: Test;
    let test2: Test;
    test2.a = 32;
    test2.b = cast<i8> 8;
    test = test2;
    if (test.b != cast<i8> 8) {
        return 1;
    }

    test2.a = 5;
    test2.b = cast<i8> 1;
    test2 = *(&test);
    if (test2.b != cast<i8> 8) {
        return 1;
    }

    if (returnUnion().b != 8) {
        return 1;
    }

    return 0;
}

fun returnUnion() -> Test {
    let test: Test;
    test.a = 32;
    test.b = cast<i8> 8;
    return test;
}
