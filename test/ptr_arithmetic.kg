extern malloc: (u64) -> ptr<void>;
extern free: (ptr<void>) -> void;

pub fun main() -> i64 {
    let i = cast<ptr<i64>> malloc(sizeof(i64) * 4);
    const malloc_addr = i;
    let num = 0;
    for (let x = 0; x < 5; x = x + 1) {
        *i = x;
        i = i + 1;
    }
    if (malloc_addr < i) {
        i = i - 5;
    }
    num = *i;
    free(cast<ptr<void>> i);
    return num;
}