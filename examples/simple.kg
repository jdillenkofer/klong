fun main() -> i64 {
    // a b c
    let i = 0;
    let sum = 0;
    while(i < 5) {
        sum = sum + fib(i);
        i = i + 1;
    }
    return sum;
}

fun fib(n : i64) -> i64 {
    if (n == 0) {
        return 0;
    }

    if (n == 1) {
        return 1;
    }
    return fib(n - 1) + fib(n - 2);
}