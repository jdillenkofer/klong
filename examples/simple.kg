main();

fun main() {
    // a b c
    const i: i64 = 2;
    let res = fib(i);
    print(res);
    return;
}

fun fib(n : i64): i64 {
    print("n == 0: " + (n == 0) + "\n");
    if (n == 0) {
        return 0;
    }

    print("n == 1: " + (n == 1) + "\n");
    if (n == 1) {
        return 1;
    }
    return fib(n - 1) + fib(n - 2);
}