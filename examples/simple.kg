fun main() -> i64 {
    // a b c
    let sum = 0;
    let funVariable = fib;
    funVariable = fibDoubled;
    for(let i = 0; i < 5; i = i + 1) {
        sum = sum + funVariable(i);
    }
    return sum % 2; // sum(0, 5, fibDoubled(n)) % 2 should return 0
}

fun fib(n : i64) -> i64 {
    if (n == 0) {
        return 0;
    }

    if (n == 1) {
        return 1;
    }
    return fib(n - 1) + fib(n - 2);
}

fun fibDoubled(n: i64) -> i64 {
    return fib(n) * 2;
}